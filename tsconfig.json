{
    "compilerOptions": {
        "target": "ESNext",
        "useDefineForClassFields": true,
        "lib": ["DOM", "DOM.Iterable", "ESNext"],
        "allowJs": false,
        "skipLibCheck": true,
        "esModuleInterop": false,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "Node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "types": ["node", "vite/client"],
        "baseUrl": ".",
        "paths": {
            "vfile": ["node_modules/vfile"],
            "vfile/*": ["node_modules/vfile/*"],
            "vfile-message": ["node_modules/vfile-message"],
            "vfile-message/*": ["node_modules/vfile-message/*"]
        }
    },
    "include": ["src", "vite-env.d.ts", "media.d.ts", "vite.config.ts"]
}
